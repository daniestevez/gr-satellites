<?xml version="1.0"?>
<block>
  <name>D-SAT Image Decoder</name>
  <key>satellites_dsat_image_decoder</key>
  <category>[Satellites]/Data</category>
  <import>import satellites</import>
  <make>satellites.dsat_image_decoder($path, $display)</make>
  <param>
    <name>Path</name>
    <key>path</key>
    <type>string</type>
  </param>
  <param>
    <name>Display</name>
    <key>display</key>
    <value>True</value>
    <type>bool</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>


  
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

</block>
